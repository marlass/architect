<div class="js-page__manageHeader">
    <script>
        window.__PRESTATE__ = {};
        window.__PRESTATE__.header = {{{json header}}};
        window.__PRESTATE__.footer = {{{json footer}}};
    </script>
    <button class="manageHeader__saveButton adminMainSaveButton"><i class="fa fa-floppy-o fa-lg" aria-hidden="true"></i>Zapisz</button>
    <h1 class="headerManagment__main-title">Nagłówek</h1>
    <div class="manageHeader headerManagment">
        <h2 class="headerManagment__title">Ogólne dane</h2>
        <p class="manageHeader__wrapper headerManagment__wrapper">
            <label class="headerManagment__label">Nazwa firmy PL</label>
            <input type="text" value="{{header.title.pl}}" class="manageHeader__title-pl headerManagment__input">
        </p>
        <p class="manageHeader__wrapper headerManagment__wrapper">
            <label class="headerManagment__label">Nazwa firmy EN</label>
            <input type="text" value="{{header.title.en}}" class="manageHeader__title-en headerManagment__input">
        </p>
        <h2 class="headerManagment__title">Nawigacja PL</h2>
        <div class="manageHeader__linksPlContainer">{{#each header.links.pl}}<div class="manageHeader__linkGroup block u-clearfix" data-block-id="{{this.blockId}}">
                    <button class="headerLinkPl__up headerManagment__up"><i class="fa fa-arrow-up" aria-hidden="true"></i></button>
                    <button class="headerLinkPl__down headerManagment__down"><i class="fa fa-arrow-down" aria-hidden="true"></i></button>
                    <button class="headerLinkPl__delete headerManagment__delete"><i class="fa fa-trash" aria-hidden="true"></i></button>
                    <p class="headerManagment__wrapper--table">
                        <label class="headerManagment__label--table">Nazwa</label>
                        <input type="text" class="manageHeader__linkPL-title headerManagment__input--table" value="{{this.title}}">
                    </p>
                    <p class="headerManagment__wrapper--table">
                        <label class="headerManagment__label--table">Adres</label>
                        <input type="text" class="manageHeader__linkPL-url headerManagment__input--table" value="{{this.url}}">
                    </p>
                </div>{{/each}}</div>
        <button type="button" class="manageHeader__add-link-pl headerManagment__addButton">Dodaj link</button>
        <h2 class="headerManagment__title">Nawigacja EN</h2>
        <div class="manageHeader__linksENContainer">{{#each header.links.en}}<div class="manageHeader__linkGroup block u-clearfix" data-block-id="{{this.blockId}}">
                    <button class="headerLinkEn__up headerManagment__up"><i class="fa fa-arrow-up" aria-hidden="true"></i></button>
                    <button class="headerLinkEn__down headerManagment__down"><i class="fa fa-arrow-down" aria-hidden="true"></i></button>
                    <button class="headerLinkEn__delete headerManagment__delete"><i class="fa fa-trash" aria-hidden="true"></i></button>
                    <p class="headerManagment__wrapper--table">
                        <label class="headerManagment__label--table">Nazwa</label>
                        <input type="text" class="manageHeader__linkEN-title headerManagment__input--table" value="{{this.title}}">
                    </p>
                    <p class="headerManagment__wrapper--table">
                        <label class="headerManagment__label--table">Adres</label>
                        <input type="text" class="manageHeader__linkEN-url headerManagment__input--table" value="{{this.url}}">
                    </p>
                </div>{{/each}}</div>
        <button type="button" class="manageHeader__add-link-en headerManagment__addButton">Dodaj link</button>
    </div>
    <h1 class="headerManagment__main-title">Stopka</h1>
    <div class="manageFooter headerManagment">
        <h2 class="headerManagment__title">Ogólne dane</h2>
        <p class="manageFooter__wrapper headerManagment__wrapper--third">
            <label class="headerManagment__label">Nazwa firmy PL</label>
            <input type="text" value="{{footer.title.pl}}" class="manageFooter__title-pl headerManagment__input">
        </p>
        <p class="manageFooter__wrapper headerManagment__wrapper--third">
            <label class="headerManagment__label">Nazwa firmy EN</label>
            <input type="text" value="{{footer.title.en}}" class="manageFooter__title-en headerManagment__input">
        </p>
        <p class="manageFooter__wrapper headerManagment__wrapper--third">
            <label class="headerManagment__label">Rok</label>
            <input type="text" value="{{footer.year}}" class="manageFooter__year headerManagment__input">
