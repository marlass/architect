                        <input type="text" class="manageFooter__linkPL-url headerManagment__input--table" value="{{this.url}}">
                    </p>
                </div>{{/each}}</div>
        <button type="button" class="manageFooter__add-link-pl headerManagment__addButton">Dodaj link</button>
        <h2 class="headerManagment__title">Nawigacja EN</h2>
        <div class="manageFooter__linksENContainer">{{#each footer.links.en}}<div class="manageFooter__linkGroup block u-clearfix" data-block-id="{{this.blockId}}">
