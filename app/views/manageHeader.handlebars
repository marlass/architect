        <h2 class="headerManagment__title">Nawigacja PL</h2>
        <div class="manageFooter__linksPlContainer">{{#each footer.links.pl}}<div class="manageFooter__linkGroup block u-clearfix" data-block-id="{{this.blockId}}">
