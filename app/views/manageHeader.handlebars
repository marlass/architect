                        <p class="headerManagment__wrapper--office u-clearfix">
                            <label class="headerManagment__label--office">E-mail</label>
                            <input type="text" class="manageFooter__officeEN-email headerManagment__input--office" value="{{this.email}}">
                        </p>
                    </div>
                </div>{{/each}}</div>
        <button type="button" class="manageFooter__add-office-en headerManagment__addButton">Dodaj biuro</button>
    </div>
